alias vi='nvim'
alias vim='nvim'

# alias ls='ls -G'
alias eza='eza --color-scale --git --icons -F'
alias ls='eza'
alias ll='eza -l'
alias lt='eza -T'
alias -- -='cd -'        # Go back

alias node='node --use_strict'

# git
alias co='git cf'
alias develop='git checkout develop'
alias master='git checkout master'

if [ -f ~/.dotfiles.local/aliases ]
then
  source ~/.dotfiles.local/aliases
fi
