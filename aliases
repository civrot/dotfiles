alias ls='ls -G'
alias ..='cd ..'         # Go up one directory
alias ...='cd ../..'     # Go up two directories
alias ....='cd ../../..' # Go up two directories
alias -- -='cd -'        # Go back

alias z='zeus'
alias zs='zeus start'

eval "$(hub alias -s)"

if [ -f ~/.dotfiles.local/aliases ]
then
  source ~/.dotfiles.local/aliases
fi
